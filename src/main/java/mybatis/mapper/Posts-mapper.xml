<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="posts">

	<!-- 특정 캠프장 찾는 용도 -->
	<select id="findByCamp" resultType="data.like" parameterType="java.lang.String">
		SELECT LIKES.*
		FROM LIKES 
		WHERE CAMPNAME = #{campname }
	</select>
</mapper>